{
    "name" : "insa-login-rememberer",
    "version" : "1.0",
    "description" : "Extension to automatically login to your INSA account.",
    "permissions": [],
    "content_scripts": [
        {
            "matches": ["https://cas.insa-rouen.fr/*", "https://moodle.insa-rouen.fr/login/index.php", "https://dsi.insa-rouen.fr/cas/"],
            "js": [
                "insa_login_rememberer.js", "run_wasm.js"
            ],
            "all_frames": false
        }
    ],
    "web_accessible_resources": [{
        "resources": ["insa_login_rememberer_bg.wasm"],
        "matches": ["https://cas.insa-rouen.fr/*", "https://moodle.insa-rouen.fr/login/index.php", "https://dsi.insa-rouen.fr/cas/"]
    }],
    "manifest_version": 3
}
